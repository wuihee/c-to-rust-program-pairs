{
    "pairs": [
        {
            "name": "eza",
            "description": "A modern alternative to ls.",
            "description_url": "https://github.com/eza-community/eza/blob/main/README.md",
            "translation": "manual",
            "features": "superset",
            "c_program": {
                "url": "https://github.com/coreutils/coreutils",
                "build": [
                    "./bootstrap",
                    "./configure",
                    "make"
                ],
                "test": [
                    "make check"
                ],
                "dependencies": [
                    "autoconf",
                    "automake",
                    "libtool",
                    "gettext"
                ],
                "source": [
                    "src/ls.c",
                    "src/ls.h"
                ],
                "executable": "src/ls"
            },
            "rust_program": {
                "url": "https://github.com/eza-community/eza",
                "build": [
                    "cargo build"
                ],
                "test": [
                    "cargo test"
                ],
                "source": [
                    "/"
                ],
                "executable": "target/release/eza"
            }
        },
